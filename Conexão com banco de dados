const Sequelize = require("sequelize");
const sequelize = new Sequelize("atividade", "root", "654321", {
  host: "localhost",
  dialect: "mysql",
});

sequelize
  .authenticate()
  .then(function () {
    console.log("Conex√£o realizada com sucesso!");
  })
  .catch(function (erro) {
    console.log("Falha ao conectar! " + erro);
  });
