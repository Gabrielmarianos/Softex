    <script>
        document.write("Testando");

document.write("<hr>");

const aluno = {
    nome: 'Gabriel',
    matricula: 123456,
    curso: 'Engenharia'
};

document.write("Propriedades do objeto aluno: <br>");


for (const propriedade in aluno){
    //document.write("${propriedade}: ${aluno[propriedade]}");
    document.write(propriedade, "<br>");
}

document.write("<hr>");

const array = [25, 14, 62, 02, 73];

document.write("Elementos do array: <br>");

for (const i of array){
    document.write(i, " ")
}
    </script>
